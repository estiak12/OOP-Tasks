// take base and height from the user and fine the area of the triangle 
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int base=sc.nextInt();
    int height=sc.nextInt();
    float val=0.5f;
    float area;
    area=val*base*height;
    System.out.println("The area is "+area);
    }
}

//find the voulme of a Ball 
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int radius=sc.nextInt();
    float val=4/3f;
    float pi=3.1416f;

    float area;
    area=val*pi*(radius*radius*radius);
    System.out.println("The area is "+area);
    }
}

// Find the area of a triangle
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int radius=sc.nextInt();
   // float val=4/3f;
    float pi=3.1416f;

    float area;
    area=pi*(radius*radius);
    System.out.println("The area is "+area);
    }
}
// convert celcius scale temperature to Farenhite scale
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int celcius=sc.nextInt();
   // float val=4/3f;
    float val=(9*celcius)/5;
    val+=32;

    System.out.println("The area is "+val);
    }
}

// find the area of a equiletarel triangle
public class EquilateralTriangleArea {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter the length of the side of the equilateral triangle: ");
        double side = scanner.nextDouble();

        double area = (Math.sqrt(3) / 4) * side * side;

        System.out.printf("Area of the equilateral triangle: %.4f\n", area);
    }
}
// A prograam that check that wheather a value is positive or negative

import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int val=sc.nextInt();
    if(val>=0)System.out.println("The value is positive ");
    else System.out.println("The value is Negative "); 
    }
}

//A prograam that check that wheather a value is divisible by 2 or not 

import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int val=sc.nextInt();
    if(val%2==0)System.out.println("The value is divisible by two ");
    else System.out.println("The value is not divisible by two "); 
    }
}

//Result making using switch case 
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int mark=sc.nextInt();
    switch(mark/10){
        case 4:System.out.println("The obtain Grade is C+");
        break;
        case 5:System.out.println("The obtain Grade is C+");
        break;
        case 6:System.out.println("The obtain Grade is B");
        break;
        case 7:System.out.println("The obtain Grade is A-");
        break;
        case 8:System.out.println("The obtain grade is A");
        break;
        case 9:System.out.println("The obtain grade is A+");
        break;
        default :System.out.println("The obtain Grade is F");
    }
    }
}

// This is the simple calculator code 
import java.util.Scanner;
public class Superclass{
    public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    char  c=sc.next().charAt(0);
    int a=sc.nextInt();
    int b=sc.nextInt();
    if(c=='+'){
        int result=(a+b);
        System.out.println(result);
    }
    else if(c=='-'){
        int result=(a-b);
        System.out.println(result);
    }
    else if(c=='*'){
        int result=a*b;
        System.out.println(result);
    }
    else {
        float result=(a/b);
        System.out.println(result);
    }
    }
}




